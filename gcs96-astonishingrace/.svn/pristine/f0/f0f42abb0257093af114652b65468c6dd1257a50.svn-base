
package model;

import javax.swing.JPanel;
import javax.swing.BorderFactory;

/**
 * 
 * @author Grant
 * @version Something 
 */

public class TimeController extends JPanel {
    
    private static final int HORIZONTAL_MARGIN = 75;
    private static final int VERTICAL_MARGIN = 75;

    /**  */
    private static final long serialVersionUID = 8634062918466606855L;
    
    /**  */
    private final TimeControls myTime;

    public TimeController(final TimeControls theTime) {
        super();
        setBorder(BorderFactory.createEmptyBorder(VERTICAL_MARGIN,
                                           HORIZONTAL_MARGIN,
                                           VERTICAL_MARGIN,
                                           HORIZONTAL_MARGIN));
        myTime = theTime;
    }
    
}
